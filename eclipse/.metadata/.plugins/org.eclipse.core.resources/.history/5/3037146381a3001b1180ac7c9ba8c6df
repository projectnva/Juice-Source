package clientname.basicgui.elements;

import clientname.basicgui.HUDElement;
import clientname.basicgui.HUDElementCategory;
import net.minecraft.client.Minecraft;

public class FPS extends HUDElement {

	public FPS() {
		super("FPS", "1.1", HUDElementCategory.INFO);
		setEnabled(true);
	}
	
	public void draw() {
		Minecraft.getMinecraft().fontRendererObj.drawString("FPS", 4, 4, -1);
	}
	
}
