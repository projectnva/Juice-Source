package clientname.basicgui.basicgui.settings;

import com.github.energizedab.pvptube.display.HUDElement;

public class Setting<Type> {

	public String name;
	public HUDElement element;
	public Type value;
	
	public boolean isBooleanSetting() {
		return this instanceof BooleanSetting;
	}
	
	public boolean isModeSetting() {
		return this instanceof ModeSetting;
	}
	
	public boolean isNumberSetting() {
		return this instanceof NumberSetting;
	}
	
	public String getName() {
		return name;
	}
	
	public HUDElement getElement() {
		return element;
	}
}
