package clientname.login;

import clientname.http.HWID;

public class UserChecker {
	
	
	public boolean isUser() {
		String hwid =  HWID.getHWID();
		
		for(User u : Voyage.getInstance().getUsers().getUsers()) {
			if(u.getHwid() == hwid) {
				return true;
			}
			else return false;
		}
		return false;

	}
	

}
