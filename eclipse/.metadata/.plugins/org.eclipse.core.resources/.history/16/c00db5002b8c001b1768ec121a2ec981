package com.orbitclient.imsoogood.ez.orbitclient.util.patchcrumbsv2;

import net.minecraft.entity.item.EntityTNTPrimed;

public class TNTSpot {

    public double x;
    public double y;
    public double z;
    public long fuseExpires;

    public TNTSpot(EntityTNTPrimed tntPrimed) {
        this.x = tntPrimed.posX;
        this.y = tntPrimed.posY;
        this.z = tntPrimed.posZ;
        this.fuseExpires = System.currentTimeMillis() + (long) (tntPrimed.fuse * 50);
    }
}
